{
  "id": "memphis",
  "name": "Memphis Milano",
  "description": "1980s Italian design movement",
  "emoji": "\ud83d\udd3a",
  "variables": {
    "--bg-primary": "#ffffff",
    "--bg-secondary": "#f0f0f0",
    "--bg-tertiary": "#fff5cc",
    "--text-primary": "#000000",
    "--text-secondary": "#0033cc",
    "--text-tertiary": "#d1458f",
    "--accent-primary": "#ffcc00",
    "--accent-secondary": "#0033cc",
    "--accent-tertiary": "#ff69b4",
    "--border-primary": "#000000",
    "--border-secondary": "#0000ff",
    "--shadow-primary": "rgba(255, 255, 0, 0.4)",
    "--shadow-secondary": "rgba(0, 0, 255, 0.3)",
    "--gradient-1": "#ffff00",
    "--gradient-2": "#0000ff",
    "--gradient-3": "#ff69b4",
    "--overlay": "rgba(255, 255, 255, 0.8)",
    "--hover-transform": "translateY(-5px) rotate(-5deg)",
    "--transition-speed": "0.3s",
    "--animation-name": "memphis-wiggle",
    "--font-primary": "'Righteous', cursive",
    "--font-secondary": "'Fredoka One', cursive"
  },
  "animations": {
    "background": "memphis-wiggle",
    "hover": "memphis-wiggle",
    "accent": "gradient-shift"
  },
  "effects": {
    "particles": false,
    "glitch": false,
    "scanlines": false,
    "shapes": true,
    "bounce": true
  },
  "fonts": {
    "primary": "Bungee",
    "secondary": "Fredoka One",
    "import": "@import url('https://fonts.googleapis.com/css2?family=Bungee+Fredoka+One:wght@400;700&display=swap');"
  }
}