{
  "id": "vaporwave",
  "name": "Vaporwave Plaza",
  "description": "90s mall aesthetic meets digital art",
  "emoji": "\ud83c\udf34",
  "variables": {
    "--bg-primary": "#ff69b4",
    "--bg-secondary": "#ff1493",
    "--bg-tertiary": "#ff82ab",
    "--text-primary": "#ffffff",
    "--text-secondary": "#40e0d0",
    "--text-tertiary": "#9370db",
    "--accent-primary": "#40e0d0",
    "--accent-secondary": "#9370db",
    "--accent-tertiary": "#ffffff",
    "--border-primary": "#40e0d0",
    "--border-secondary": "#9370db",
    "--shadow-primary": "rgba(64, 224, 208, 0.5)",
    "--shadow-secondary": "rgba(147, 112, 219, 0.3)",
    "--gradient-1": "#ff69b4",
    "--gradient-2": "#40e0d0",
    "--gradient-3": "#9370db",
    "--overlay": "rgba(255, 105, 180, 0.7)",
    "--hover-transform": "translateY(-4px) skew(2deg)",
    "--transition-speed": "0.4s",
    "--animation-name": "vaporwave-glitch",
    "--font-primary": "'VT323', monospace",
    "--font-secondary": "'Press Start 2P', cursive"
  },
  "animations": {
    "background": "vaporwave-glitch",
    "hover": "vaporwave-glitch",
    "accent": "gradient-shift"
  },
  "effects": {
    "particles": true,
    "glitch": true,
    "scanlines": true,
    "vhsEffect": true
  },
  "fonts": {
    "primary": "VT323",
    "secondary": "Press Start 2P",
    "import": "@import url('https://fonts.googleapis.com/css2?family=VT323+Press+Start+2P:wght@400;700&display=swap');"
  }
}