{
  "id": "artdeco",
  "name": "Art Deco Metropolis",
  "description": "1920s New York glamour",
  "emoji": "\ud83c\udfdb\ufe0f",
  "variables": {
    "--bg-primary": "#000000",
    "--bg-secondary": "#1a1a1a",
    "--bg-tertiary": "#2d2d2d",
    "--text-primary": "#d4af37",
    "--text-secondary": "#fffff0",
    "--text-tertiary": "#c0c0c0",
    "--accent-primary": "#d4af37",
    "--accent-secondary": "#004225",
    "--accent-tertiary": "#fffff0",
    "--border-primary": "#d4af37",
    "--border-secondary": "#004225",
    "--shadow-primary": "rgba(212, 175, 55, 0.5)",
    "--shadow-secondary": "rgba(0, 66, 37, 0.3)",
    "--gradient-1": "#d4af37",
    "--gradient-2": "#004225",
    "--gradient-3": "#000000",
    "--overlay": "rgba(0, 0, 0, 0.8)",
    "--hover-transform": "translateY(-3px) skew(-1deg)",
    "--transition-speed": "0.4s",
    "--animation-name": "deco-sunburst",
    "--font-primary": "'Poiret One', cursive",
    "--font-secondary": "'Josefin Sans', sans-serif"
  },
  "animations": {
    "background": "deco-sunburst",
    "hover": "deco-sunburst",
    "accent": "gradient-shift"
  },
  "effects": {
    "particles": false,
    "glitch": false,
    "scanlines": false,
    "geometric": true,
    "gold": true
  },
  "fonts": {
    "primary": "Poiret One",
    "secondary": "Josefin Sans",
    "import": "@import url('https://fonts.googleapis.com/css2?family=Poiret+One+Josefin+Sans:wght@400;700&display=swap');"
  }
}