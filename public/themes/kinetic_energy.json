{
  "id": "kinetic",
  "name": "Kinetic Energy",
  "description": "Motion design and energy visualization",
  "emoji": "\u26a1",
  "variables": {
    "--bg-primary": "#000000",
    "--bg-secondary": "#1a1a1a",
    "--bg-tertiary": "#2a2a2a",
    "--text-primary": "#ccff00",
    "--text-secondary": "#ff0066",
    "--text-tertiary": "#00ffff",
    "--accent-primary": "#ccff00",
    "--accent-secondary": "#ff0066",
    "--accent-tertiary": "#9400d3",
    "--border-primary": "#ccff00",
    "--border-secondary": "#ff0066",
    "--shadow-primary": "rgba(204, 255, 0, 0.6)",
    "--shadow-secondary": "rgba(255, 0, 102, 0.4)",
    "--gradient-1": "#ccff00",
    "--gradient-2": "#ff0066",
    "--gradient-3": "#00ffff",
    "--overlay": "rgba(0, 0, 0, 0.8)",
    "--hover-transform": "translateY(-6px) skew(-3deg)",
    "--transition-speed": "0.2s",
    "--animation-name": "energy-flow",
    "--font-primary": "'Michroma', sans-serif",
    "--font-secondary": "'Exo', sans-serif"
  },
  "animations": {
    "background": "energy-flow",
    "hover": "energy-flow",
    "accent": "gradient-shift"
  },
  "effects": {
    "particles": true,
    "glitch": true,
    "scanlines": false,
    "electricity": true,
    "motion": true
  },
  "fonts": {
    "primary": "Michroma",
    "secondary": "Electrolize",
    "import": "@import url('https://fonts.googleapis.com/css2?family=Michroma+Electrolize:wght@400;700&display=swap');"
  }
}