{
  "id": "ocean",
  "name": "Deep Ocean Abyss",
  "description": "Underwater bioluminescent theme",
  "emoji": "\ud83c\udf0a",
  "variables": {
    "--bg-primary": "#001f3f",
    "--bg-secondary": "#003366",
    "--bg-tertiary": "#004080",
    "--text-primary": "#00ffff",
    "--text-secondary": "#40e0d0",
    "--text-tertiary": "#7fffd4",
    "--accent-primary": "#00ffff",
    "--accent-secondary": "#4a148c",
    "--accent-tertiary": "#7b68ee",
    "--border-primary": "#00ffff",
    "--border-secondary": "#004080",
    "--shadow-primary": "rgba(0, 255, 255, 0.4)",
    "--shadow-secondary": "rgba(74, 20, 140, 0.3)",
    "--gradient-1": "#001f3f",
    "--gradient-2": "#00ffff",
    "--gradient-3": "#4a148c",
    "--overlay": "rgba(0, 31, 63, 0.85)",
    "--hover-transform": "translateY(-4px) scale(1.01)",
    "--transition-speed": "0.6s",
    "--animation-name": "ocean-wave",
    "--font-primary": "'Comfortaa', cursive",
    "--font-secondary": "'Quicksand', sans-serif"
  },
  "animations": {
    "background": "ocean-wave",
    "hover": "ocean-wave",
    "accent": "gradient-shift"
  },
  "effects": {
    "particles": true,
    "glitch": false,
    "scanlines": false,
    "bubbles": true,
    "waves": true
  },
  "fonts": {
    "primary": "Montserrat",
    "secondary": "Open Sans",
    "import": "@import url('https://fonts.googleapis.com/css2?family=Montserrat+Open+Sans:wght@400;700&display=swap');"
  }
}